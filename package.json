{
    "name": "tatami-fe",
    "version": "0.0.0",
    "main": "electron/dist/main.js",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "electron-build-dev": "ng build --base-href ./ && tsc --p electron && electron .",
        "electron-build-prod": "ng build --prod --base-href ./ && tsc --p electron && electron .",
        "package-macos": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
        "package-win32": "electron-packager . test --overwrite --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=ObjectMethod --version-string.ProductName=\"Electron App - Test\"",
        "package-win64": "electron-packager . test --overwrite --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=ObjectMethod --version-string.ProductName=\"Electron App - Test\"",
        "package-linux": "electron-packager . test --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.2.5",
        "@angular/cdk": "~8.2.3",
        "@angular/common": "~8.2.5",
        "@angular/compiler": "~8.2.5",
        "@angular/core": "~8.2.5",
        "@angular/forms": "~8.2.5",
        "@angular/material": "~8.2.3",
        "@angular/platform-browser": "~8.2.5",
        "@angular/platform-browser-dynamic": "~8.2.5",
        "@angular/router": "~8.2.5",
        "@fortawesome/fontawesome-free": "^5.6.0",
        "@kolkov/angular-editor": "^1.1.3",
        "@ng-bootstrap/ng-bootstrap": "^5.1.4",
        "@types/chart.js": "^2.8.4",
        "@types/jquery": "^3.5.0",
        "animate.css": "^3.7.2",
        "bootstrap": "^4.4.1",
        "bootstrap-icons": "^1.5.0",
        "chart.js": "^2.5.0",
        "core-js": "^2.5.4",
        "easy-pie-chart": "^2.1.7",
        "file-saver": "^2.0.2",
        "hammerjs": "^2.0.8",
        "jquery": "^3.5.1",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "ng5-slider": "^1.2.4",
        "ngx-device-detector": "^1.3.14",
        "ngx-electron": "^2.2.0",
        "ngx-permissions": "^7.0.3",
        "rxjs": "~6.5.3",
        "screenfull": "3.3.0",
        "tslib": "^1.9.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.3",
        "@angular/cli": "~8.3.3",
        "@angular/compiler-cli": "~8.2.5",
        "@angular/language-service": "~8.2.5",
        "@types/electron": "^1.6.10",
        "@types/node": "~8.9.4",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "codelyzer": "^5.0.1",
        "electron": "^5.0.13",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.0",
        "tslint": "~5.11.0",
        "typescript": "~3.5.3"
    }
}
